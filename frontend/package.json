{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "courses.demo",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "output": "electron-build"
    }
  },
  "dependencies": {
    "chart.js": "^2.7.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "electron-pack": "react-scripts build && build --em.main=build/electron.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "electron": "^1.7.10",
    "electron-builder": "^19.53.6"
  }
}
